@CHARSET "ISO-8859-1";
@import "style-global.css";


/**header footer global setting**/
#wrapper{
	margin:3px auto;
	text-align:center;
	width: 1080px;
}
#header, footer{
	height: 170px;
}
#header{
	margin: 0 0 10px 0;
	background: url("../images/jpg-header-banner.jpg") no-repeat bottom;
}
#header h1 { 
	float: left;
	background: url("../images/png-epsilon.png")  no-repeat; 
	width: 233px;
	height: 69px; 
	text-indent: -9999px;
	margin: 20px 0 0 200px; 
}
#header div{
	clear:both;
	text-align: left;
}
#header .welcome{	
	float: right;
	margin: 17px 90px;
	padding: 0 15px;
	/**background: url("../images/gif-arrow.gif") no-repeat right;8**/
}
.bar{
	height:32px;
}
#header .home{
	clear:both;
	position: relative;
    top: 8px;
    left: 100px;

	padding: 8px 13px;
	width: 100px;
	height:32px;
	background-color:#bedade;
}
#header .logout{
	display: block;
	float:right;
    position: relative;
    top: -31px;	
	width: 150px;
	height:40px;	
	background-color:#bedade;
}
#left{
	float: left;
	margin: 10px 10px;
	width: 250px;
	border: 1px solid #f5f5f5;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;	
	background-color:#f5f5f5;	
}
nav{
	folat:clean;
	text-align: center;	
	margin: 0 auto;
}
#left nav a:link,nav a:visited{ 
	color:black;
	text-align: center;	
}
nav a{
	display:block;
	font-size: bold;
	left:10px;
	padding: 10px 5px;
	margin: 15px;
	border: 1px solid #8ecfeb;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;	
	background-color:#8ecfeb;
}
nav  .clicked{
	background-color:#46b5e4;
}
/**********************right **********************************/
#right{
	margin-left: 300px;
	margin-top:10px;
	padding: 10px 50px;
	background-color: white;
	border: 2px solid #5a96e4;
	-moz-border-radius: 3px;
  	-webkit-border-radius: 3px;	
	border-radius: 3px;
	text-align:left;
	font-size: 14px;
}
#right h6{
	font-size: 16px;
}
.fm{
	padding: 10px 50px;
	margin: 10px auto;

}
.fm form{
	clear: left;
	padding: 10px 50px;
	text-align: left;
	margin: 5px auto;
	
}
input{
	position:relative;
	left:1px;
	padding: 8px;
	margin: 8px;
	width: 200px;
	border: 1px solid #d8d8d8;
	-moz-border-radius: 10px;
  	-webkit-border-radius: 10px;
  	border-radius: 10px;
}
 .search{
    width:35px;
    height:30px;
    border:1px solid #d8d8d8;
	-moz-border-radius: 4px;
  	-webkit-border-radius: 4px;
  	border-radius: 4px;    
    background: url("../images/gif-search.gif")  no-repeat;
}
.star{
	color:red;
}
.form-group span{
	width: 150px;
	padding:10px;
	
}
table, th, td {
    border: 1px solid black;
}
table {
    width: 100%;
    border-collapse: collapse;
}

th {
    height: 50px;
    text-align: center;
}
td {
    height: 30px;
    vertical-align: center;
}
.addBtn {
  font-family: arial;
  color: black !important;
  font-size: bold;
  width:auto;
  text-shadow: 1px 1px 0px #7CACDE;
  box-shadow: 1px 1px 1px #BEE2F9;
  padding: 10px 25px;
  margin:10px 20px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  border: 1px solid #3866A3;
  background: #63B8EE;
  background: linear-gradient(top,  #63B8EE,  #468CCF);
  background: -ms-linear-gradient(top,  #63B8EE,  #468CCF);
  background: -webkit-gradient(linear, left top, left bottom, from(#63B8EE), to(#468CCF));
  background: -moz-linear-gradient(top,  #63B8EE,  #468CCF);
}
.addBtn:hover {
  color: #14396A !important;
  background: #468CCF;
  background: linear-gradient(top,  #468CCF,  #63B8EE);
  background: -ms-linear-gradient(top,  #468CCF,  #63B8EE);
  background: -webkit-gradient(linear, left top, left bottom, from(#468CCF), to(#63B8EE));
  background: -moz-linear-gradient(top,  #468CCF,  #63B8EE);
}
footer{
	background: url("../images/footer.jpg") no-repeat;
	
}
footer p{
	float:right;
	font-size:10px;
	padding: 15px 200px 1px 0;

}
